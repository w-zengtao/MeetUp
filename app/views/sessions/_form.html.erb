<%= form_for :session, url: :sessions do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="form-group">
    <%= f.label :password %>
    <%= f.text_field :password %>
  </div>

  <div class="form-group">
    <%= f.submit '登录' %>
  </div>
<% end %>
